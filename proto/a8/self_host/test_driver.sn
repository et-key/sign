#_print_tokens : toks ?
    toks = 0 : 0
    tok : head toks
    type : head tok
    
    type = -1 : (
        print_str_ln "EOF"
        0
    )
    
    val : head (tail tok)
    `If type is ID, Num, Str, Op - val is string/num`
    `If type is Punc - val is char code`
    
    print_str "Tok: "
    print_num type
    print_str " Val: "
    
    type = 5 : print_char val
    type != 5 : print_str val
    
    print_char 10
